{
  "env": {
      "es2021": true,
      "node": true
  },
  "extends": [
      "google"
  ],
  "parserOptions": {
      "ecmaVersion": 12
  },
  "rules": {
      "semi": "off",
      "linebreak-style": ["error", "windows"],
      "object-curly-spacing": ["error", "always"],
      "max-len": ["error", { "code": 100 }],
      "array-bracket-spacing": ["error", "always"],
      "no-console": "warn"
  }
}
